{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) Launch lifter",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/bazel-bin/recompilation/lifter/lifter",
      "args": [
        "--loaded_elf_filename", "recompilation/loader/multiple_functions.elf",
        "--bb_head_addresses_filename", "recompilation/disassembler/addresses.txt"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "sourceFileMap" : {"/proc/self/cwd/": "${workspaceFolder}/bazel-uwin"},
      "preLaunchTask": "buildLifterDebug",
    }
  ]
}